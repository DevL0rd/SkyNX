{
    "Lua.diagnostics.disable": [
        "lowercase-global",
        "undefined-doc-name"
    ],
    "Lua.diagnostics.globals": [
        "__simulator",
        "print",
        "simulator"
    ],
    "Lua.runtime.version": "Lua 5.3",
    "Lua.runtime.builtin": {
        "coroutine": "enable",
        "bit32": "enable",
        "bit": "enable",
        "builtin": "enable",
        "utf8": "enable",
        "package": "enable",
        "os": "enable",
        "jit": "enable",
        "io": "enable",
        "ffi": "enable",
        "debug": "enable",
        "basic": "enable"
    }
}